{
  new():: {
  },
  withConstantSignal(constant_signal):: {
    constant_signal: constant_signal,
  },
  withConstantSignalMixin(constant_signal):: {
    constant_signal+: constant_signal,
  },
  withSignalName(signal_name):: {
    signal_name: signal_name,
  },
  withSignalNameMixin(signal_name):: {
    signal_name+: signal_name,
  },
}
