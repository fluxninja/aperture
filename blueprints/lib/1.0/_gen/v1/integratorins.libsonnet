  },
  withInputMixin(input):: {
    input+: input,
  },
  withMax(max):: {
    max: max,
  },
  withMaxMixin(max):: {
    max+: max,
  },
  withMin(min):: {
    min: min,
  },
  withMinMixin(min):: {
    min+: min,
  },
