{
  new():: {
  },
  withLabelMatcher(label_matcher):: {
    label_matcher: label_matcher,
  },
  withLabelMatcherMixin(label_matcher):: {
    label_matcher+: label_matcher,
  },
  withName(name):: {
    name: name,
  },
  withNameMixin(name):: {
    name+: name,
  },
  withParameters(parameters):: {
    parameters: parameters,
  },
  withParametersMixin(parameters):: {
    parameters+: parameters,
  },
}
