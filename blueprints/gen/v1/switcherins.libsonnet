{
  new():: {
  },
  withOffSignal(off_signal):: {
    off_signal: off_signal,
  },
  withOffSignalMixin(off_signal):: {
    off_signal+: off_signal,
  },
  withOnSignal(on_signal):: {
    on_signal: on_signal,
  },
  withOnSignalMixin(on_signal):: {
    on_signal+: on_signal,
  },
  withSwitch(switch):: {
    switch: switch,
  },
  withSwitchMixin(switch):: {
    switch+: switch,
  },
}
