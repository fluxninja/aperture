{
  new():: {
  },
  withDuration(duration):: {
    duration: duration,
  },
  withDurationMixin(duration):: {
    duration+: duration,
  },
  withTargetOutput(target_output):: {
    target_output: target_output,
  },
  withTargetOutputMixin(target_output):: {
    target_output+: target_output,
  },
}
