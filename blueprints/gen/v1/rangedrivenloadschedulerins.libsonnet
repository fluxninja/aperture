{
  new():: {
  },
  withOverloadConfirmation(overload_confirmation):: {
    overload_confirmation: overload_confirmation,
  },
  withOverloadConfirmationMixin(overload_confirmation):: {
    overload_confirmation+: overload_confirmation,
  },
  withSignal(signal):: {
    signal: signal,
  },
  withSignalMixin(signal):: {
    signal+: signal,
  },
}
