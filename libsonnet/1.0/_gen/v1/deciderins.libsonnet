{
  new():: {
  },
  withLhs(lhs):: {
    lhs: lhs,
  },
  withLhsMixin(lhs):: {
    lhs+: lhs,
  },
  withOnFalse(on_false):: {
    on_false: on_false,
  },
  withOnFalseMixin(on_false):: {
    on_false+: on_false,
  },
  withOnTrue(on_true):: {
    on_true: on_true,
  },
  withOnTrueMixin(on_true):: {
    on_true+: on_true,
  },
  withRhs(rhs):: {
    rhs: rhs,
  },
  withRhsMixin(rhs):: {
    rhs+: rhs,
  },
}
