{
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "controller-prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "axisLabel": "",
            "fillOpacity": 10
          },
          "unit": ""
        }
      },
      "gridPos": {
        "h": 10,
        "w": 24
      },
      "id": 1,
      "interval": "30s",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "controller-prometheus"
          },
          "expr": "increase(signal_reading_sum{policy_name=\"signal-processing\",signal_name=\"${signal_name}\",sub_circuit_id=\"${sub_circuit_id}\"}[$__rate_interval]) / increase(signal_reading_count{policy_name=\"signal-processing\",signal_name=\"${signal_name}\",sub_circuit_id=\"${sub_circuit_id}\"}[$__rate_interval])",
          "intervalFactor": 1
        }
      ],
      "title": "Signal Average - ${signal_name} (${sub_circuit_id})",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "controller-prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "axisLabel": "",
            "fillOpacity": 10
          },
          "unit": ""
        }
      },
      "gridPos": {
        "h": 10,
        "w": 24
      },
      "id": 2,
      "interval": "30s",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "controller-prometheus"
          },
          "expr": "avg(rate(signal_reading_count{policy_name=\"signal-processing\",signal_name=\"${signal_name}\",sub_circuit_id=\"${sub_circuit_id}\"}[$__rate_interval]))",
          "intervalFactor": 1,
          "legendFormat": "Valid"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "controller-prometheus"
          },
          "expr": "sum(rate(invalid_signal_readings_total{policy_name=\"signal-processing\",signal_name=\"${signal_name}\",sub_circuit_id=\"${sub_circuit_id}\"}[$__rate_interval]))",
          "intervalFactor": 1,
          "legendFormat": "Invalid"
        }
      ],
      "title": "Signal Validity (Frequency) - ${signal_name} (${sub_circuit_id})",
      "type": "timeseries"
    }
  ],
  "refresh": "5s",
  "templating": {
    "list": [
      {
        "includeAll": false,
        "label": "Data Source",
        "multi": false,
        "name": "datasource",
        "query": "prometheus",
        "type": "datasource"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "includeAll": false,
        "label": "Signal Name",
        "multi": false,
        "name": "signal_name",
        "query": "label_values(signal_reading{policy_name=\"signal-processing\"}, signal_name)",
        "sort": 1,
        "type": "query"
      },
      {
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "includeAll": false,
        "label": "Sub Circuit ID",
        "multi": false,
        "name": "sub_circuit_id",
        "query": "label_values(signal_reading{policy_name=\"signal-processing\",signal_name=\"${signal_name}\"}, sub_circuit_id)",
        "sort": 1,
        "type": "query"
      }
    ]
  },
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timezone": "browser",
  "title": "Aperture Dashboard"
}
