{
  "logLevel": "debug",
  "theme": "base",
  "themeVariables": {
      "primaryColor": "#8EC4AD",
      "primaryTextColor": "#20242C",
      "primaryBorderColor": "#20242C",
      "secondaryColor": "#e5e5e5",
      "secondaryTextColor": "#20242C",
      "tertiaryColor": "#0000001a",
      "tertiaryTextColor": "#20242C",
      "lineColor": "#20242C"
  },
  "background": "transparent",
  "themeCSS": ".node rect { fill: #FC9A69; } span.edgeLabel { padding: 0.1em }",
  "startOnLoad": true,
  "flowchart": {
    "useMaxWidth": false,
    "htmlLabels": true
  }
}
