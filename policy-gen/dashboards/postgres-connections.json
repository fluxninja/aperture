{
    "panels": [
        {
            "datasource": {
                "type": "prometheus",
                "uid": "$datasource"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Decisions",
                        "fillOpacity": 10
                    },
                    "unit": "reqps"
                }
            },
            "gridPos": {
                "h": 10,
                "w": 24,
                "y": 10
            },
            "id": 1,
            "interval": "30s",
            "targets": [
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "$datasource"
                    },
                    "expr": "sum by(decision_type) (rate(workload_requests_total{policy_name=\"postgres-connections\"}[$__rate_interval]))",
                    "intervalFactor": 1
                }
            ],
            "title": "Quota Checks",
            "type": "timeseries"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "$datasource"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Decisions",
                        "fillOpacity": 10
                    },
                    "unit": "reqps"
                }
            },
            "gridPos": {
                "h": 10,
                "w": 24,
                "y": 20
            },
            "id": 2,
            "interval": "30s",
            "targets": [
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "$datasource"
                    },
                    "expr": "sum by(workload_index, decision_type) (rate(workload_requests_total{policy_name=\"postgres-connections\",decision_type=\"DECISION_TYPE_ACCEPTED\"}[$__rate_interval]))",
                    "intervalFactor": 1
                }
            ],
            "title": "Workload Decisions (accepted)",
            "type": "timeseries"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "$datasource"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Decisions",
                        "fillOpacity": 10
                    },
                    "unit": "reqps"
                }
            },
            "gridPos": {
                "h": 10,
                "w": 24,
                "y": 30
            },
            "id": 3,
            "interval": "30s",
            "targets": [
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "$datasource"
                    },
                    "expr": "sum by(workload_index, decision_type) (rate(workload_requests_total{policy_name=\"postgres-connections\",decision_type=\"DECISION_TYPE_REJECTED\"}[$__rate_interval]))",
                    "intervalFactor": 1
                }
            ],
            "title": "Workload Decisions (rejected)",
            "type": "timeseries"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "$datasource"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Latency",
                        "fillOpacity": 10
                    },
                    "unit": "ms"
                }
            },
            "gridPos": {
                "h": 10,
                "w": 24,
                "y": 40
            },
            "id": 4,
            "interval": "30s",
            "targets": [
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "$datasource"
                    },
                    "expr": "(sum by (workload_index) (increase(workload_latency_ms_sum{policy_name=\"postgres-connections\"}[$__rate_interval])))/(sum by (workload_index) (increase(workload_latency_ms_count{policy_name=\"postgres-connections\"}[$__rate_interval])))",
                    "intervalFactor": 1
                }
            ],
            "title": "Workload Latency",
            "type": "timeseries"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "$datasource"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Wait Time",
                        "fillOpacity": 10
                    },
                    "unit": "ms"
                }
            },
            "gridPos": {
                "h": 10,
                "w": 24,
                "y": 50
            },
            "id": 5,
            "interval": "30s",
            "targets": [
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "$datasource"
                    },
                    "expr": "(sum by (component_id) (increase(request_in_queue_duration_ms_sum{policy_name=\"postgres-connections\"}[$__rate_interval])))/(sum by (component_id) (increase(request_in_queue_duration_ms_count{policy_name=\"postgres-connections\"}[$__rate_interval])))",
                    "intervalFactor": 1
                }
            ],
            "title": "Request in Queue Duration",
            "type": "timeseries"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "$datasource"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisLabel": "Token Rate",
                        "fillOpacity": 10
                    },
                    "unit": ""
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "y": 60
            },
            "id": 6,
            "interval": "30s",
            "targets": [
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "$datasource"
